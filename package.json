		"cssnano": "^5.0.17",
		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"postcss": "^8.4.6",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
